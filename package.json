{
  "name": "demetrius1-plugin",
  "version": "1.0.0",
  "description": "Awesome starter plugin example",
  "author": "Demetrius Vissarion <demetrius.vissarion'gmail.com> (https://github.com/demetriusvissarion)",
  "repository": {
    "type": "git",
    "url": "https://github.com/demetriusvissarion/blog_site2-wordpress-plugin1"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "PHP",
    "composer",
    "es6",
    "scss"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "babel-loader": "^9.1.3",
    "babelify": "^10.0.0",
    "browser-sync": "^2.29.3",
    "browserify": "^17.0.0",
    "browserify-shim": "^3.8.16",
    "npm-watch": "^0.11.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "scripts": {
    "sass": "sass src/scss/mystyle.scss assets/mystyle.css",
    "build": "webpack --mode production",
    "watch": "webpack --config assets/webpack.config.js --mode=development --watch",
    "dev": "webpack --config assets/webpack.config.js"
  },
  "watch": {
    "sass": {
      "patterns": [
        "src/scss"
      ],
      "extensions": [
        "scss"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "babelify": "^10.0.0",
    "browser-sync": "^2.29.3",
    "browserify": "^17.0.0",
    "browserify-shim": "^3.8.16",
    "npm-watch": "^0.11.0"
  }
}
